{
  "name": "@kubernetes-models/gke",
  "version": "2.0.0",
  "description": "Kubernetes GKE models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/packages/gke",
  "author": "Rueian <rueiancsie@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate",
    "postbuild": "publish-scripts postbuild",
    "prepack": "publish-scripts prepack",
    "clean": "rimraf gen"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "gke",
    "google-cloud"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kubernetes-models/base": "workspace:*",
    "@kubernetes-models/validate": "workspace:*",
    "kubernetes-models": "workspace:*",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:*",
    "@kubernetes-models/publish-scripts": "workspace:*",
    "rimraf": "^3.0.2"
  },
  "crd-generate": {
    "input": [
      "./crd/backendconfigs.yaml",
      "https://raw.githubusercontent.com/GoogleCloudPlatform/k8s-config-connector/v1.58.0/install-bundles/install-bundle-workload-identity/crds.yaml"
    ],
    "output": "./gen"
  }
}
