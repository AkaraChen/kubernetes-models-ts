{
  "name": "@kubernetes-models/contour",
  "version": "2.0.0",
  "description": "Contour models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/packages/contour",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate",
    "postbuild": "publish-scripts postbuild",
    "prepack": "publish-scripts prepack",
    "clean": "rimraf gen"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "contour"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kubernetes-models/base": "workspace:*",
    "@kubernetes-models/validate": "workspace:*",
    "kubernetes-models": "workspace:*",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:*",
    "@kubernetes-models/publish-scripts": "workspace:*",
    "rimraf": "^3.0.2"
  },
  "crd-generate": {
    "input": [
      "https://raw.githubusercontent.com/projectcontour/contour/v1.5.0/examples/contour/01-crds.yaml",
      "https://raw.githubusercontent.com/projectcontour/contour/v1.18.0/examples/contour/01-crds.yaml"
    ],
    "output": "./gen"
  }
}
