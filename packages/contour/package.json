{
  "name": "@kubernetes-models/contour",
  "version": "1.0.1",
  "description": "Contour models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:v1beta1": "crd-generate --input https://raw.githubusercontent.com/projectcontour/contour/v1.5.0/examples/contour/01-crds.yaml --output ./gen",
    "build:v1": "crd-generate --input https://raw.githubusercontent.com/projectcontour/contour/v1.9.0/examples/contour/01-crds.yaml --output ./gen",
    "prepack": "cp package.json README.md dist/",
    "clean": "rimraf gen"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "kubernetes",
    "contour"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@kubernetes-models/base": "^1.0.0",
    "@kubernetes-models/validate": "^1.0.0",
    "kubernetes-models": "^1.0.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
