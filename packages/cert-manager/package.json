{
  "name": "@kubernetes-models/cert-manager",
  "version": "1.0.3",
  "description": "cert-manager models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:v1alpha1": "crd-generate --input https://raw.githubusercontent.com/jetstack/cert-manager/v0.10.1/deploy/manifests/00-crds.yaml --output ./gen",
    "build:v1alpha2": "crd-generate --input https://raw.githubusercontent.com/jetstack/cert-manager/v0.14.3/deploy/manifests/00-crds.yaml --output ./gen",
    "build:v1beta1": "crd-generate --input https://github.com/jetstack/cert-manager/releases/download/v0.16.1/cert-manager.crds.yaml --output ./gen",
    "build:v1": "crd-generate --input https://github.com/jetstack/cert-manager/releases/download/v1.0.3/cert-manager.crds.yaml --output ./gen",
    "prepack": "cp package.json README.md dist/",
    "clean": "rimraf gen"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "kubernetes",
    "cert-manager"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@kubernetes-models/base": "^1.1.0",
    "@kubernetes-models/validate": "^1.0.1",
    "kubernetes-models": "^1.0.3",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
