{
  "name": "kubernetes-models",
  "version": "1.0.3",
  "description": "Kubernetes models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "openapi-generate --input https://raw.githubusercontent.com/kubernetes/kubernetes/v1.19.2/api/openapi-spec/swagger.json --output ./gen",
    "prepack": "cp package.json README.md dist/",
    "clean": "rimraf gen"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "kubernetes"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@kubernetes-models/base": "^1.1.0",
    "@kubernetes-models/validate": "^1.0.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@kubernetes-models/openapi-generate": "^1.1.0",
    "rimraf": "^3.0.2"
  }
}
