{
  "name": "@kubernetes-models/crd-generate",
  "version": "2.0.0",
  "description": "Generate Kubernetes models for custom resource definitions (CRD).",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/packages/crd-generate",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "bin": {
    "crd-generate": "bin/crd-generate.cjs"
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "crd"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/bin/",
    "/dist/"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kubernetes-models/generate": "workspace:*",
    "@kubernetes-models/read-input": "workspace:*",
    "@kubernetes-models/string-util": "workspace:*",
    "lodash": "^4.17.21",
    "make-dir": "^3.1.0",
    "tslib": "^2.3.0",
    "yaml": "^1.10.2",
    "yargs": "^17.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/yargs": "^17.0.2"
  }
}
