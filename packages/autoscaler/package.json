{
  "name": "@kubernetes-models/autoscaler",
  "version": "0.2.3",
  "description": "Kubernetes Autoscaler models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/packages/autoscaler",
  "author": "Rueian <rueiancsie@gmail.com>",
  "license": "MIT",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:vpa-v1alpha1": "crd-generate --input https://raw.githubusercontent.com/kubernetes/autoscaler/vertical-pod-autoscaler/v0.9.2/vertical-pod-autoscaler/deploy/vpa-crd.yaml --output ./gen",
    "build:vpa-v1beta1": "crd-generate --input https://raw.githubusercontent.com/kubernetes/autoscaler/vertical-pod-autoscaler/v0.9.2/vertical-pod-autoscaler/deploy/vpa-beta-crd.yaml --output ./gen",
    "build:vpa-v1beta2": "crd-generate --input https://raw.githubusercontent.com/kubernetes/autoscaler/vertical-pod-autoscaler/v0.9.2/vertical-pod-autoscaler/deploy/vpa-beta2-crd.yaml --output ./gen",
    "build:vpa-v1": "crd-generate --input https://raw.githubusercontent.com/kubernetes/autoscaler/vertical-pod-autoscaler/v0.9.2/vertical-pod-autoscaler/deploy/vpa-v1-crd.yaml --output ./gen",
    "postbuild": "publish-scripts postbuild",
    "prepack": "publish-scripts prepack",
    "clean": "rimraf gen"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "autoscaler",
    "vpa"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kubernetes-models/base": "workspace:*",
    "@kubernetes-models/validate": "workspace:*",
    "kubernetes-models": "workspace:*",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:*",
    "@kubernetes-models/publish-scripts": "workspace:*",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
