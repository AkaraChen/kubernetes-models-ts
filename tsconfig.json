{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "preserveConstEnums": true,
    "strict": true,
    "strictNullChecks": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "lib": ["es2018"],
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "kubernetes-models/*": ["packages/kubernetes-models/dist/*"]
    }
  }
}
