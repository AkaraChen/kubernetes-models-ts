{
  "name": "@kubernetes-models/cert-manager",
  "version": "4.3.1",
  "description": "cert-manager models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/cert-manager",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "go-crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "cert-manager"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@kubernetes-models/apimachinery": "workspace:^",
    "@kubernetes-models/base": "workspace:^",
    "@kubernetes-models/validate": "workspace:^",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@kubernetes-models/go-crd-generate": "workspace:^",
    "@kubernetes-models/publish-scripts": "workspace:^",
    "vitest": "^0.29.8"
  },
  "go-crd-generate": {
    "input": [
      {
        "module": "github.com/cert-manager/cert-manager@v0.10.1",
        "packages": [
          "./pkg/apis/certmanager/v1alpha1"
        ]
      },
      {
        "module": "github.com/cert-manager/cert-manager@v1.6.3",
        "packages": [
          "./pkg/apis/certmanager/v1alpha2",
          "./pkg/apis/certmanager/v1alpha3",
          "./pkg/apis/certmanager/v1beta1"
        ]
      },
      {
        "module": "github.com/cert-manager/cert-manager@v1.11.2",
        "packages": [
          "./pkg/apis/certmanager/v1"
        ]
      },
      {
        "module": "github.com/cert-manager/trust-manager@v0.4.0",
        "packages": [
          "./pkg/apis/trust/v1alpha1"
        ]
      },
      {
        "module": "github.com/cert-manager/approver-policy@v0.6.3",
        "packages": [
          "./pkg/apis/policy/v1alpha1"
        ]
      },
      {
        "module": "github.com/cert-manager/aws-privateca-issuer@v1.2.2",
        "packages": [
          "./pkg/api/v1beta1"
        ]
      }
    ],
    "output": "./gen"
  }
}
